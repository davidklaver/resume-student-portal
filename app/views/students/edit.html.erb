<h2>Here's a form to edit your info:</h2>
<%= form_tag "/students/#{current_user.id}", method: :patch do %>
	<p><%= label_tag :first_name %>
	<%= text_field_tag :first_name, @student["firstName"] %></p>
	<p><%= label_tag :last_name %>
	<%= text_field_tag :last_name, @student["lastName"] %></p>
	<p><%= label_tag :email %>
	<%= text_field_tag :email, @student["email"] %></p>
	<p><%= label_tag :linkedin_url %>
	<%= text_field_tag :linkedin_url, @student["linkedinUrl"] %></p>
	<p><%= label_tag :twitter_handle %>
	<%= text_field_tag :twitter_handle, @student["twitterHandle"] %></p>
	<p><%= label_tag :personal_website_url %>
	<%= text_field_tag :personal_website_url, @student["personalWebsiteUrl"] %></p>
	<p><%= label_tag :online_resume_url %>
	<%= text_field_tag :online_resume_url, @student["onlineResumeUrl"] %></p>
	<p><%= label_tag :github_url %>
	<%= text_field_tag :github_url, @student["githubUrl"] %></p>
	<p><%= label_tag :photo %>
	<%= text_field_tag :photo, @student["photo"] %></p>
	<p><%= label_tag :phone_number %>
	<%= text_field_tag :phone_number, @student["phoneNumber"] %></p>
	<p><%= label_tag :bio %>
	<%= text_area_tag :bio, @student["bio"] %></p>

	<p><%= submit_tag 'Update Info' %></p>
<% end %>